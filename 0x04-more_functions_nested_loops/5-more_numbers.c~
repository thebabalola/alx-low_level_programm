#include "main.h"

/**
 * more_numbers - This function prints numbers from 0 to 14 ten times.
 * There is no return value.
 */

void more_numbers(void)
{
	int iteration, digit;

	for (iteration = 0; iteration < 10; iteration++)
	{
		for (digit = 0; digit < 15; digit++)
		{
			if (digit >= 10)
				_putchar((digit / 10) + '0');
			_putchar((digit % 10) + '0');
		}

		_putchar('\n');
	}
}
