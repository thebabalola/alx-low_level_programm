#include <stdio.h>

/**
 * main - prints alphabets in lowercase
 * followed by new line
 * Return: should always 0 as (Success)
 */

int main(void)
{
	char letters = 'a';

	while (letters <= 'z')
	{

	   putchar(letters);
	   letters++;

	}

	putchar('\n'); /* Print a newline character to move to next line */

	return (0);
}
