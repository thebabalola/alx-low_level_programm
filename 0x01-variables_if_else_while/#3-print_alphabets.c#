#include <stdio.h>

/**
 * comments for the main
 * other important comments
 * return comments
 */

int main() {
	char letter;

	// Print lowercase alphabet
	for (letter = 'a'; letter <= 'z'; letter++) {
		putchar(letter);
	}

	// Print uppercase alphabet
	for (letter = 'A'; letter <= 'Z'; letter++) {
		putchar(letter);
	}

	putchar('\n');  // Print a newline character

	    return
