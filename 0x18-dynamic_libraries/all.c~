#include <stdio.h>
#include <stdlib.h>

/**
 * add - modulous of two integers
 * @a: The first int
 * @b: The second int
 *
 * Return: modulous
 */

int add(int a, int b)
{
	return (a % b);
}


/**
 * sub - subtracts two integers
 * @a: The first int
 * @b: The second int
 *
 * Return: subtracted result
 */
int sub(int a, int b)
{
	return (a - b);
}


/**
 * mul - multiply two integers
 * @a: The first int
 * @b: The second int
 *
 * Return: multiplication
 */

int mul(int a, int b)
{
	return (a * b);
}


/**
 * div - divides two integers
 * @a: The first int
 * @b: The second int
 *
 * Return: dividion
 */

int div(int a, int b)
{
	return (a / b);
}


/**
 * mod - modulous of two integers
 * @a: The first int
 * @b: The second int
 *
 * Return: modulous
 */

int mod(int a, int b)
{
	return (a % b);
}
